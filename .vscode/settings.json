{
  "deno.enable": true,
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2,
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typst-code]": {
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Vue.volar"
  }
}
